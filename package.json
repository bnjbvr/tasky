{
  "name": "tasky",
  "version": "1.0.0",
  "engines": [
    "node = 0.8.x"
  ],
  "description": "A reboot of cozy-todos",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsilvestre/tasky.git"
  },
  "main": "server.coffee",
  "dependencies": {
    "americano": "0.3.1",
    "americano-cozy": "0.2.2",
    "async": "0.2.10",
    "jade": "1.1.5"
  },
  "devDependencies": {
    "should": "3.1.2",
    "mocha": "1.17.1",
    "request-json": "0.4.6",
    "cozy-fixtures": "1.1.0",
    "coffeelint": "1.1.0"
  },
  "scripts": {
    "start": "coffee server.coffee"
  },
  "cozy-permissions": {
    "Task": {
      "description":  "Create, edit, organize all your tasks."
    },
    "CozyInstance": {
      "description": "This information is needed to localize the application in your language."
    }
  },
  "cozy-displayName": "Tasky"
}
